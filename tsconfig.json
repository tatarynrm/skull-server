{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./types", "./node_modules/@types","./src/bot/types"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}